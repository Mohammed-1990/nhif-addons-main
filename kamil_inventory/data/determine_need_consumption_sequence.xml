<odoo>
	<data >

		<record id="determine_need_sequence" model="ir.sequence">
			<field name="name">new request</field>
			<field name="code">determine.need.seq</field>
			<field name="prefix">DN</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>
		<record id="determine_consumption_sequence" model="ir.sequence">
			<field name="name">new request</field>
			<field name="code">determine.consumption.seq</field>
			<field name="prefix">DC</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>

		<record id="indirect-receive_sequence" model="ir.sequence">
			<field name="name">Indirect Receive Sequence</field>
			<field name="code">indirect.receive.sequence</field>
			<field name="prefix">IND</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>


	</data>
</odoo>