<odoo>
    <data noupdate="0">

        <!-- sequence for Employees Picking -->
        <record id="emp_picking_sequence" model="ir.sequence">
            <field name="name">Emplyees Picking</field>
            <field name="code">employees.picking</field>
            <field name="prefix">Need Request/ </field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Employees Location -->
         <record id="stock_location_employees" model="stock.location">
            <field name="name">Employees Locations</field>
            <field name="location_id" ref="stock.stock_location_locations_partner"/>
            <field name="usage">employees</field>
            <field name="company_id"></field>
        </record>

        <!-- Employees Requests Picking type -->
        <record id="stock_picking_employees" model="stock.picking.type">
            <field name="name">Employees Requests</field>
            <field name="code">outgoing</field>
            <field name="for_emloyees_request">1</field>
            <field name="sequence_id" ref="emp_picking_sequence"></field>
        </record>
    </data>
</odoo>