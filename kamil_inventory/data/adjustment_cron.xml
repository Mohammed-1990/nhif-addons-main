<odoo>
    <data noupdate="1">

        <record id="cron_annual_auto_adjust" model="ir.cron">
            <field name="interval_type">months</field>
            <field name="name">Annual Inventory Adjustment</field>
            <field name="numbercall">-1</field>
            <field name="priority">5</field>
            <field name="doall">False</field>
            <field name="active">True</field>
            <field name="interval_number">12</field>
            <field name="model_id" ref="model_auto_adjust"></field>
            <field name="state">code</field>
            <field name="code">model.create_annual_adjust()</field>
            <field name="nextcall" >2019-12-31 21:59:59</field>
        </record>

        <record id="cron_periodic_auto_adjust" model="ir.cron">
            <field name="interval_type">months</field>
            <field name="name">Periodic Inventory Adjustment</field>
            <field name="numbercall">-1</field>
            <field name="priority">5</field>
            <field name="doall">False</field>
            <field name="active">True</field>
            <field name="interval_number">1</field>
            <field name="model_id" ref="model_auto_adjust"></field>
            <field name="state">code</field>
            <field name="code">model.create_periodic_adjust()</field>
        </record>
    </data>
</odoo>