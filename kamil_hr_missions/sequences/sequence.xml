<?xml version="1.0" ?>
<odoo>
    <data noupdate='1'>
		
         <record id="ir_seq_missions_assigned" model="ir.sequence">
            <field name="name">Missions Assigned</field>
            <field name="code">missions.assigned.req</field>
            <field name="prefix">مأمورية-%(year)s-%(month)s-</field>
            <field name="padding">5</field>
        </record>

         <record id="ir_seq_mission_petty" model="ir.sequence">
            <field name="name">Mission petty</field>
            <field name="code">mission.petty.req</field>
            <field name="prefix">نثرية-%(year)s-%(month)s-</field>
            <field name="padding">5</field>
        </record>

        <record id="ir_seq_mission_filtering" model="ir.sequence">
            <field name="name">Mission filtering</field>
            <field name="code">mission.filtering.req</field>
            <field name="prefix">تصفية-النثرية-%(year)s-%(month)s-</field>
            <field name="padding">5</field>
        </record>

	</data>
</odoo>