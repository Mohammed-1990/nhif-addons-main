<odoo>
	<data noupdate="1">
		<record id="ir_seq_hr_contravention" model="ir.sequence">
			<field name="name">Contravention</field>
			<field name="code">sequence.hr.contravention</field>
			<field name="prefix">%(year)s-%(month)s-</field>
			<field name="padding">5</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
		</record>
		
		<record id="ir_seq_work_injuries" model="ir.sequence">
			<field name="name">Work Injuriess</field>
			<field name="code">sequence.work.injuries</field>
			<field name="prefix">%(year)s-%(month)s-</field>
			<field name="padding">5</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
		</record>
	</data>
</odoo>