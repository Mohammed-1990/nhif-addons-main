<?xml version="1.0" encoding="utf-8"?>
<odoo>
	

	<!-- Form Wiz Revenues analysis subgroups report -->
	<record id="view_wiz_revenues_analysis_subgroups_form" model="ir.ui.view">
		<field name="name">wiz.revenues.analysis.subgroups.form</field>
		<field name="model">wiz.revenues.analysis.subgroups</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group col="4">
						<field name="date_from"/>
						<field name="date_to"/>
						<field name="group_id" options="{'no_create':'1'}" required="1"/>
					</group>
					<footer>
						<button name="print_report" type="object" string="print" class="btn-primary"/>
						<button string="Cancel" special="cancel" class="btn-secondary"/>
					</footer>
				</sheet>
			</form>
		</field>
	</record>



	<!-- Wiz Rwiz revenues analysis subgroups-->
	<act_window
		id="action_wiz_revenues_analysis_subgroups"
		name="Revenues Analysis Subgroups Report"
		res_model="wiz.revenues.analysis.subgroups"
		view_mode="form"
		target="new"
	/>



		<menuitem 
		id="menu_wiz_revenues_analysis_subgroups" 
		sequence="6"
		parent="kamil_accounting_reports.menu_accounting_report"
	 	action="action_wiz_revenues_analysis_subgroups"/>


</odoo>