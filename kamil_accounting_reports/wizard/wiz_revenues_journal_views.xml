<?xml version="1.0" encoding="utf-8"?>
<odoo>
	

	<!-- Form Wiz Ratification report -->
	<record id="view_wiz_revenues_journal_form" model="ir.ui.view">
		<field name="name">wiz.revenues.journal.form</field>
		<field name="model">wiz.revenues.journal</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group col="4">
						<field name="date_from"/>
						<field name="date_to"/>
						<field name="group_id" options="{'no_create':'1'}" required="1" string="Choose Account/Group"/>
					</group>
					<footer>
						<button name="print_report" type="object" string="print" class="btn-primary"/>
						<button string="Cancel" special="cancel" class="btn-secondary"/>
					</footer>
				</sheet>
			</form>
		</field>
	</record>



	<!-- Wiz Revenues Journal-->
	<act_window
		id="action_wiz_revenues_journal"
		name="Revenues Journa Report"
		res_model="wiz.revenues.journal"
		view_mode="form"
		target="new"
	/>



		<menuitem 
		id="menu_revenues_journal" 
		sequence="5"
		parent="kamil_accounting_reports.menu_accounting_report"
	 	action="action_wiz_revenues_journal"/>


</odoo>