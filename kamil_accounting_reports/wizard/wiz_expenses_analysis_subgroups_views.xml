<?xml version="1.0" encoding="utf-8"?>
<odoo>
	

	<!-- Form Wiz expenses analysis subgroups report -->
	<record id="view_wiz_expenses_analysis_subgroups_form" model="ir.ui.view">
		<field name="name">wiz.expenses.analysis.subgroups.form</field>
		<field name="model">wiz.expenses.analysis.subgroups</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group col="4">
						<field name="date_from"/>
						<field name="date_to"/>
						<field name="group_id" options="{'no_create':'1'}" required="1"/>
					</group>
					<footer>
						<button name="print_report" type="object" string="print" class="btn-primary"/>
						<button string="Cancel" special="cancel" class="btn-secondary"/>
					</footer>
				</sheet>
			</form>
		</field>
	</record>



	<!-- Wiz Rwiz expenses analysis subgroups-->
	<act_window
		id="action_wiz_expenses_analysis_subgroups"
		name="Expenses Analysis Subgroups Report"
		res_model="wiz.expenses.analysis.subgroups"
		view_mode="form"
		target="new"
	/>



		<menuitem 
		id="menu_wiz_expenses_analysis_subgroups" 
		sequence="9"
		parent="kamil_accounting_reports.menu_report_payment"
	 	action="action_wiz_expenses_analysis_subgroups"/>


</odoo>