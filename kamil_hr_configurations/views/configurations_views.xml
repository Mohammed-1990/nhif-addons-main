        <!-- hr function -->
   <record model="ir.ui.view" id="hr_function_form_view">
      <field name="name">hr_function.form</field>
      <field name="model">hr.function</field>
      <field name="arch" type="xml">
          <form>
            <sheet>
              <div>
                  <h1>
                    <field name="name" placeholder="Function Name"/>
                  </h1>
               </div>

              <notebook>
              <page string="Job Requirements">
                <div>
                  <field name="job_requirements"/>
                </div>
              </page>
              <page string="Functional Tasks">
                <field name="task_line_ids">
                  <tree editable="bottom">
                    <field name="task_id" options="{'no_create':True,'no_edit':True,'no_open':True}"/>
                  </tree>
                </field>
              </page>

               <page string="Competencies">
                <field name="competencie_line_ids">
                  <tree  editable="bottom">
                    <field name="competencie_id" options="{'no_create':True,'no_edit':True,'no_open':True}"/>
                    <field name="required_degree"/>
                  </tree>
                </field>
              </page >
            </notebook>

            </sheet>
            <div class="oe_chatter">
              <field name="message_follower_ids" widget="mail_followers"/>
              <field name="activity_ids" widget="mail_activity"/>
              <field name="message_ids" widget="mail_thread"/>
          </div> 
          </form>
        </field>
      </record>


