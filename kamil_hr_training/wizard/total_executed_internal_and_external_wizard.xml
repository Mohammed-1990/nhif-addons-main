<odoo>
	<data>
		<record id="total_executed_internal_and_external_wizard_form" model="ir.ui.view">
			<field name='name'>total executed internal and external wizard</field>
			<field name='model'>total.executed.internal.and.external.wizard</field>
			<field name='arch' type='xml'>
    			<form>
    			    <sheet>
            	    <group col="4">
                        
                           <field name="date_from" autocomplete="off"/>
                           <field name="date_to" autocomplete="off"/>
                           <field name="country" options="{'no_create':True,'no_edit':True,'no_open':True}"/>
                           <field name="training_type" options="{'no_create':True,'no_edit':True,'no_open':True}"/>
                        
                    </group>
                      <footer>
                          <button name='get_report' string='Print' type='object' class="oe_highlight"/>
                          <button string="Cancel" special="cancel"/>
                      </footer>
        			</sheet>
                </form>
    	    </field>
       </record>





    <record id="action_total_executed_internal_and_external_wizard" model="ir.actions.act_window">
        <field name="name">Total Executed Internal And External report</field>
        <field name="res_model">total.executed.internal.and.external.wizard</field>
        <field name="view_type">form</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
   </record>

    <menuitem name="Total Executed report"
              id="menu_total_executed_internal_and_external"
              parent="short_taining_menu"
              action="action_total_executed_internal_and_external_wizard"
              sequence = "4"
              />

	</data>
</odoo>
