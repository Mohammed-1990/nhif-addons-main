<?xml version="1.0" ?>
<odoo>
    <data noupdate='1'>
		

		<record id="ir_seq_hr_loan" model="ir.sequence">
            <field name="name">Loan Request</field>
            <field name="code">hr.loan.req</field>
            <field name="prefix">%(year)s-%(month)s-</field>
            <field name="padding">5</field>
        </record>

        <record id="ir_seq_hr_incentive" model="ir.sequence">
            <field name="name">Incentive Request</field>
            <field name="code">hr.incentive.req</field>
            <field name="prefix">%(year)s-%(month)s-</field>
            <field name="padding">3</field>
        </record>
        

	</data>
</odoo>