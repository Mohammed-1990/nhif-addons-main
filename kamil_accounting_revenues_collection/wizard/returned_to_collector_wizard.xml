<odoo>
    <data>
        <record id="record_return_receipt_e15_wizard_form" model="ir.ui.view">
            <field name='name'>Return Receipt e15</field>
            <field name='model'>returned.collector.e15.wizard</field>
            <field name='arch' type='xml'>
                <form>
                    <sheet>
                          
                        <group>
                            <field name="returned_reason"/>
                            <field name ="receipt_67_id" readonly="1"/>
                        </group>
                        <footer>
                            <button name='returned_collector_receipt_e15' string='Send request' type='object'
                                    class="oe_highlight" confirm="انت تقوم بارجاع اورنيك 67 . هل انت متاكد ؟"/>
                            <button string="Cancel" special="cancel"/>
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_cancel_receipt_e15_wizard" model="ir.actions.act_window">
            <field name='name'>Return Receipt e15</field>
            <field name="res_model">returned.collector.e15.wizard</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>


    </data>
</odoo>