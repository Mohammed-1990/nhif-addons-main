    <record model="ir.ui.view" id="kamil_contracts_services_form_view">
         <field name="name">Service Price</field>
         <field name="model">kamil.contracts.services_price</field>      
        <field name="arch" type="xml">
           <form string="Services Price">
             <sheet>
             <group>
                 <field name="service_code" string="Service Code"/> 
                 <field name="service_scode" string="Scode"/>
               <field name="e_service_name" string="English Service name"  />

               <field name="a_service_name" string="إسم الخدمة"  />
            </group>
            <group>
               <field name="service_price_for_public" string="Service Price for Public"/>
               <field name="service_price_for_employees" string="Service Price for Employees"/>
               
            </group>

           </sheet>
           <div class="oe_chatter">
                  <field name="message_follower_ids" widget="mail_followers"/>
                  <field name="activity_ids" widget="mail_activity"/>
                  <field name="message_ids" widget="mail_thread"/>
               </div>
           </form>
         </field>
      </record>
<!--medical group form view-->
      <record model="ir.ui.view" id="medical_group_form_view">
         <field name="name">Medical Group</field>
         <field name="model">medical.group</field>      
           <field name="arch" type="xml">
              <form string="Medical Group">
                <sheet>
                <group>
                  <field name="MainGroup"/>
