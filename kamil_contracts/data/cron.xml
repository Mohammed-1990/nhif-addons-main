<?xml version="1.0" encoding='UTF-8'?>
<odoo>

    <record id="contract_end_date_cron" model="ir.cron">
        <field name="name">Archive Expired Contracts</field>
        <field name="model_id" ref="model_kamil_contracts_contract"/>
        <field name="state">code</field>
        <field name="code">model.check_contract_end_date()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>

    </record>

    <record id="contract_start_date_cron" model="ir.cron">
        <field name="name">Start Contracts</field>
        <field name="model_id" ref="model_kamil_contracts_contract"/>
        <field name="state">code</field>
        <field name="code">model.check_contract_start_date()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>

    </record>

</odoo>