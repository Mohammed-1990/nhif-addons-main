<odoo>
   

    <report id="technical_performance_report"
            model="technical.performance.wizard"
            string="Tchnical Performance Report"
            report_type="qweb-html"
            name="kamil_hr_career.technical_performance_report_view"
            paperformat="kamil_hr_career.paperformat_career_landscape"
            menu="False"/>

    <template id="technical_performance_report_view">
        <t t-call="web.html_container">
        <t t-call="web.external_layout">
            
            <div class="page">
                <div class="text-center" style="font:500;">
                <br/> <br/>
                <p>تقرير الاداء الفني</p>
                       من تاريخ : <span t-esc="date_from"/> الي تاريخ :<span t-esc="date_to"/>
                </div><br/>
                <t t-if="docs!=[]">
                    <table style="margin:10px;width:100%;" dir="rtl" class="table-bordered">
                        <thead style="display: table-header-group;">
                            <tr class="table-active">
                              <th class="text-center fit">اسم الموظف</th>
                              <t t-foreach="evaluations" t-as="evaluation">
                                  <th class="text-center fit"><span t-esc="evaluation"/></th>
                              </t>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-foreach="docs" t-as="doc">
                                <tr>
                                    <td class="text-center">
                                        <span t-esc="doc['employee']"/>
                                    </td>
                                    <t t-foreach="doc['evaluation']" t-as="eval">
                                        <td class="text-center">
                                            <span t-esc="eval"/>
                                        </td>
                                    </t>
                                   
                                </tr>
                            </t>
                        </tbody>
                    </table>
                </t>
                <t t-if="docs==[]">
                    <br/><br/><br/><br/>
                    <div class="text-center"><h1>لا تـوجـــد بيــانات</h1></div>
                </t>
            </div>
            </t>
        </t>
    </template>
</odoo>