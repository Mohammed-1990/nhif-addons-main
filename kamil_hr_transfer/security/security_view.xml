<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="model_internal_transfer_comp_rule" model="ir.rule">
            <field name="name">internal transfer multi company rule</field>
            <field name="model_id" ref="model_internal_transfer"/>
            <field name="global" eval="True"/>
            <field name="domain_force">['|',('employee_id.company_id','=',False),('employee_id.company_id','child_of',[user.company_id.id])]</field>
        </record>

        <record id="model_final_transfer_comp_rule" model="ir.rule">
            <field name="name">final transfer multi company rule</field>
            <field name="model_id" ref="model_final_transfer"/>
            <field name="global" eval="True"/>
            <field name="domain_force">['|',('branch_id','=',False),('branch_id','child_of',[user.company_id.id])]</field>
        </record> 
    </data>
</odoo>
