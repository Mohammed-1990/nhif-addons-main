<odoo>
	<data>

		<menuitem id="hide_menu" parent="base.next_id_9" />

		<menuitem id="account.menu_finance_receivables" groups="kamil_accounting_base.kamil_accounting_hide_group" parent="hide_menu"/>

		<menuitem id="account.menu_finance_payables" groups="kamil_accounting_base.kamil_accounting_hide_group" parent="hide_menu"/>
		
		<menuitem id="account.menu_action_tax_form" groups="kamil_accounting_base.kamil_accounting_hide_group" parent="hide_menu"/>

		<menuitem id="account.menu_action_account_fiscal_position_form" groups="kamil_accounting_base.kamil_accounting_hide_group" parent="hide_menu"/>

		<menuitem id="account.menu_action_incoterm_open" groups="kamil_accounting_base.kamil_accounting_hide_group" parent="hide_menu"/>

		<menuitem id="account_accountant.account_tag_menu" groups="kamil_accounting_base.kamil_accounting_hide_group" parent="hide_menu"/>

		<menuitem id="account_accountant.menu_account_group" groups="kamil_accounting_base.kamil_accounting_hide_group" parent="hide_menu"/>

		<menuitem id="account.action_account_reconcile_model_menu" groups="kamil_accounting_base.kamil_accounting_hide_group" parent="hide_menu"/>

		<menuitem id="account.menu_action_payment_term_form" groups="kamil_accounting_base.kamil_accounting_hide_group" parent="hide_menu"/>

		<menuitem id="account_reports_followup.account_followup_menu" groups="kamil_accounting_base.kamil_accounting_hide_group" parent="hide_menu"/>

		<menuitem id="account_budget.menu_budget_post_form" groups="kamil_accounting_base.kamil_accounting_hide_group" parent="hide_menu"/>

		<menuitem id="account.account_analytic_group_menu" groups="kamil_accounting_base.kamil_accounting_hide_group" parent="hide_menu"/>


		<record id="tax_form_inherit" model="ir.ui.view">
			<field name="name">Inherit Tax Form</field>
			<field name="model">account.tax</field>
			<field name="inherit_id" ref="account.view_tax_form"/>
			<field name="arch" type="xml">
				<field name="account_id" position="attributes">
					<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
					</attribute>
				</field>
				<field name="refund_account_id" position="attributes">
					<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
					</attribute>
				</field>
			</field>
		</record>


		<record id="cost_center_in_analytic_form" model="ir.ui.view">
			<field name="name">Cost Center in analytic</field>
			<field name="model">account.analytic.line</field>
			<field name="inherit_id" ref="analytic.view_account_analytic_line_form"/>
			<field name="arch" type="xml">
				<group name="analytic_entry" position="inside">
					<field name="cost_center_id"/>
				</group>
			</field>
		</record>



		<record id="cost_center_tree_view" model="ir.ui.view">
			<field name="name">Cost Centers Tree</field>
			<field name="model">kamil.account.cost.center</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name"/>
				</tree>
			</field>
		</record>


		<record id="cost_center_form_view" model="ir.ui.view">
			<field name="name">Cost Centers Form</field>
			<field name="model">kamil.account.cost.center</field>
			<field name="arch" type="xml">
				<form>
					<sheet>
						<group>
							<field name="name" required="1"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="action_cost_center" model="ir.actions.act_window">
			<field name="name">Cost Centers</field>
			<field name="res_model">kamil.account.cost.center</field>
			<field name="view_mode">tree,form</field>
		</record>

		<menuitem id="cost_center_menu" name="Cost Centers" parent="account.menu_analytic_accounting" action="action_cost_center"/>




		<menuitem id="accounting_operations_menu" parent="account.menu_finance_entries" name="Operations" sequence="20"/>



		<record id="product_form" model="ir.ui.view">
			<field name="name">Product Form</field>
			<field name="model">product.template</field>
			<field name="inherit_id" ref="account.product_template_form_view"/>
			<field name="arch" type="xml">
				<page name="invoicing" position="inside">
					<group>
						<field name="budget_item_id" options="{'no_create':True,'no_edit':True,'no_open':True}"/>
					</group>
				</page>
			</field>
		</record>



		<record id="location_form" model="ir.ui.view">
			<field name="name">Location Form</field>
			<field name="model">stock.location</field>
			<field name="inherit_id" ref="stock.view_location_form"/>
			<field name="arch" type="xml">
				<form position="inside">
					<group string="Accounting Information">
						<field name="account_id" options="{'no_create':True,'no_edit':True,'no_open':True}"/>
					</group>
				</form>
			</field>
		</record>

		


		<record id="product_category_form" model="ir.ui.view">
			<field name="name">Product Category Form</field>
			<field name="model">product.category</field>
			<field name="inherit_id" ref="account.view_category_property_form"/>
			<field name="arch" type="xml">
				<field name="property_account_expense_categ_id" position="after">
					<field name="budget_item_id" options="{'no_create':True,'no_edit':True,'no_open':True}"/>
				</field>
				<field name="property_account_income_categ_id" position="attributes">
					<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
					</attribute>
				</field>
				<field name="property_account_expense_categ_id" position="attributes">
					<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
					</attribute>
				</field>
			</field>
		</record>

		<record id="view_category_property_form_inherit" model="ir.ui.view">
			<field name="name">Product Category Form</field>
			<field name="model">product.category</field>
			<field name="inherit_id" ref="purchase.view_category_property_form"/>
			<field name="arch" type="xml">
				<field name="property_account_creditor_price_difference_categ" position="attributes">
					<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
					</attribute>
				</field>
			</field>
		</record>


		<record id="view_category_property_form_inherit2" model="ir.ui.view">
			<field name="name">Product Category Form</field>
			<field name="model">product.category</field>
			<field name="inherit_id" ref="stock_account.view_category_property_form"/>
			<field name="arch" type="xml">
				<field name="property_stock_account_input_categ_id" position="attributes">
					<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
					</attribute>
				</field>
				<field name="property_stock_account_output_categ_id" position="attributes">
					<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
					</attribute>
				</field>
				<field name="property_stock_valuation_account_id" position="attributes">
					<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
					</attribute>
				</field>
				<field name="property_stock_journal" position="attributes">
					<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
					</attribute>
				</field>
			</field>
		</record>


		<record id="account_move_form" model="ir.ui.view">
			<field name="name">Account Move Form</field>
			<field name="model">account.move</field>
			<field name="inherit_id" ref="account.view_move_form"/>
			<field name="arch" type="xml">

				<notebook position="before">
					<group>
						<field name="is_manual_move" readonly="True"/>
					</group>
				</notebook>

				<field name="ref" position="attributes">
					<attribute name="string">Description</attribute>
				</field>


				<xpath expr="//notebook/page[1]/field/tree" position="attributes">
					<attribute name="limit">1000</attribute>
				</xpath>

				<xpath expr="//notebook/page[1]/field/tree/field[@name='name']" position="attributes">
					<attribute name="string">
						Description
					</attribute>
				</xpath>

				<xpath expr="//notebook/page[1]/field/tree/field[@name='account_id']" position="attributes">
					<attribute name="domain">
						[('company_id', '=', parent.company_id), ('deprecated', '=', False),('is_group','=','sub_account')]
					</attribute>
					<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
					</attribute>
				</xpath>

				<xpath expr="//notebook/page[1]/field/tree/field[@name='partner_id']" position="attributes">
					<attribute name="string">
						Partner
					</attribute>
				</xpath>

				<xpath expr="//notebook/page[1]/field/tree/field[@name='analytic_account_id']" position="attributes">
					<attribute name="string">
						Budget Item
					</attribute>
					<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
					</attribute>
				</xpath>

				<field name="journal_id" position="attributes">
					<attribute name="string">
						Journal Entry
					</attribute>
					<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
					</attribute>
				</field>
			</field>
		</record>
			
		
		<record id="account_move_tree" model="ir.ui.view">
			<field name="name">Account Move Tree</field>
			<field name="model">account.move</field>
			<field name="inherit_id" ref="account.view_move_tree"/>
			<field name="arch" type="xml">
				<field name="partner_id" position="attributes">
					<attribute name="string">Partner</attribute>
				</field>
				<field name="ref" position="attributes">
					<attribute name="string">Description</attribute>
				</field>
				
			</field>
		</record>



	<record id="account_account_form_lucky" model="ir.ui.view">
		<field name="name">account.account.form.lucky</field>
		<field name="model">account.account</field>
		<field name="inherit_id" ref="account.view_account_form"/>
		<field name="arch" type="xml">
			<field name="tax_ids" position="before">
				<!-- <field name="budget_item_id"/>				 -->
				<field name="is_group" widget="radio" options="{'horizontal': true}"/>
				
				<field name="parent_budget_item_id"/>
				<field name="clarification_number"/>
				<field name="is_inventory_account"/>
			</field>

			<field name="tax_ids" position="attributes">
				<attribute name="invisible">1</attribute>
			</field>
			
			<field name="tag_ids" position="attributes">
				<attribute name="invisible">1</attribute>
			</field>

			<field name="currency_id" position="attributes">
				<attribute name="invisible">1</attribute>
			</field>
			
			
		</field>
	</record>


	<record id="partner_form_view" model="ir.ui.view">
		<field name="name">res.partner</field>
		<field name="model">res.partner</field>
		<field name="inherit_id" ref="account.view_partner_property_form"/>
		<field name="arch" type="xml">
			<group name="banks" position="before">
				<group>
					<field name="bank_name"/>
					<field name="account_number"/>
					<field name="bank_branch_name"/>
					<field name="code"/>
				</group>
			</group>
			<group name="banks" position="attributes">
				<attribute name="invisible">1</attribute>
			</group>
			<field name="property_account_payable_id" position="attributes">
				<attribute name="required">0</attribute>
			</field>
			<field name="property_account_receivable_id" position="attributes">
				<attribute name="required">0</attribute>
			</field>
			<field name="property_account_receivable_id" position="attributes">
				<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
				</attribute>
			</field>
			<field name="property_account_payable_id" position="attributes">
				<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
				</attribute>
			</field>
		</field>
	</record>
	
	


	<record id="account_journal_form" model="ir.ui.view">
		<field name="name">account.journal.form</field>
		<field name="model">account.journal</field>
		<field name="inherit_id" ref="account.view_account_journal_form"/>
		<field name="arch" type="xml">
			<notebook position="before">
				<group>
					<field name="full_name"/>
					<field name="account_number"/>
				</group>
			</notebook>
			<field name="default_debit_account_id" position="attributes">
				<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
				</attribute>
			</field>
			<field name="default_credit_account_id" position="attributes">
				<attribute name="options">{'no_create':True,'no_edit':True,'no_open':True}
				</attribute>
			</field>
		</field>
	</record>


	<record id="action_tax_form" model="ir.actions.act_window">
	    <field name="name">Deductions/Taxes</field>
	    <field name="res_model">account.tax</field>
	    <field name="view_type">form</field>
	    <field name="view_mode">tree,kanban,form</field>
	    <!-- <field name="view_id" ref="view_tax_tree"/> -->
	    <field name="help" type="html">
	      <p class="o_view_nocontent_smiling_face">
	        Create a new tax
	      </p>
	    </field>
	</record>	

	<menuitem action="action_tax_form" id="menu_action_tax_form" parent="account.account_account_menu" sequence="2" name="Deductions/Taxes"/>


	<record id="branch_account_tree" model="ir.ui.view">
		<field name="name">branch.account.tree</field>
		<field name="model">branch.account</field>
		<field name="arch" type="xml">
			<tree>
				<field name="company_id" string="The Company"/>
			</tree>
		</field>
	</record>


	<record id="branch_account_form" model="ir.ui.view">
		<field name="name">branch.account.form</field>
		<field name="model">branch.account</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<div class="float-right">
						<h3 class="float-right">
							<label for="company_id" string="The Company" class="float-right"/> 
							<br/>
							<field name="company_id" required="1" options="{'no_create':True,'no_edit':True,'no_open':True}"/>
						</h3>
					</div>
					<br/>
					<notebook>
						<page string="Details">
							<field name="company_ids">
								<tree editable="bottom">
									<field name="company_id" options="{'no_create':True,'no_edit':True,'no_open':True}" required="1" string="The Company"/>
									<field name="account_id" options="{'no_create':True,'no_edit':True,'no_open':True}" required="1"/>
								</tree>
							</field>
						</page>
					</notebook>

				</sheet>
			</form>
		</field>
	</record>


	<record id="action_branchs_accounts" model="ir.actions.act_window">
		<field name="name">Branches Accounts</field>
		<field name="res_model">branch.account</field>
		<field name="view_mode">tree,form</field>
	</record>
	
	<menuitem id="branch_account_menu" parent="account.account_account_menu" action="action_branchs_accounts" name="Branchs Accounts" sequence="0"/>


	<record id="analytic_line_tree" model="ir.ui.view">
		<field name="name">analytic.line.tree</field>
		<field name="model">account.analytic.line</field>
		<field name="inherit_id" ref="analytic.view_account_analytic_line_tree"/>
		<field name="arch" type="xml">
			<field name="unit_amount" position="attributes">
				<attribute name="invisible">1</attribute>
			</field>
		</field>
	</record>


	<record id="analytic_line_tree2" model="ir.ui.view">
		<field name="name">analytic.line.tree</field>
		<field name="model">account.analytic.line</field>
		<field name="inherit_id" ref="account.view_account_analytic_line_tree_inherit_account"/>
		<field name="arch" type="xml">
			<field name="ref" position="attributes">
				<attribute name="invisible">1</attribute>
			</field>
		</field>
	</record>

	<record id="account.action_move_journal_line" model="ir.actions.act_window">
            <field name="name">Journal Entries</field>
            <field name="res_model">account.move</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,kanban,form</field>
            <field name="view_id" ref="account.view_move_tree"/>
            <field name="search_view_id" ref="account.view_account_move_filter"/>
            <field name="context">{'default_is_manual_move' : False, 'is_manual_move':False, 'view_no_maturity': True,'create' : False,'edit':False}</field>
            <field name="domain">[('is_manual_move','=',False)]</field>
            <field name="help" type="html">
              <p class="o_view_nocontent_smiling_face">
                Create a journal entry
              </p><p>
                A journal entry consists of several journal items, each of
                which is either a debit or a credit transaction.
              </p>
            </field>
        </record>

	<record id="action_manual_moves" model="ir.actions.act_window">
		<field name="name">Manual Moves</field>
		<field name="res_model">account.move</field>
		<field name="domain">[('is_manual_move','=',True)]</field>
		<field name="context">{'default_is_manual_move' : True, 'is_manual_move':True}</field>
	</record>

	<menuitem id="menu_manual_moves" parent="account.menu_finance_entries_accounting_entries" name="Manual Moves" action="action_manual_moves" sequence="-10"/>



	<record id="action_all_moves" model="ir.actions.act_window">
		<field name="name">All Moves</field>
		<field name="res_model">account.move</field>
		<field name="context">{'create' : False,'edit':False}</field>
	</record>

	<menuitem id="menu_all_moves" parent="account.menu_finance_entries_accounting_entries" name="All Moves" action="action_all_moves" sequence="-5"/>






	</data>
</odoo>
