<odoo>
	<data noupdate="1">

	    <record id="ir_seq_hr_warning" model="ir.sequence">
	        <field name="name">warnings</field>
	        <field name="code">warning.sequence</field>
	        <field name="prefix">%(year)s-%(month)s-</field>
	        <field name="padding">5</field>
	        <field name="number_increment">1</field>
	        <field name="number_next_actual">1</field>
	        <field name="implementation">standard</field>
	    </record>

	    <record id="ir_seq_hr_investigation" model="ir.sequence">
	        <field name="name">investigations</field>
	        <field name="code">investigation.sequence</field>
	        <field name="prefix">%(year)s-%(month)s-</field>
	        <field name="padding">5</field>
	        <field name="number_increment">1</field>
	        <field name="number_next_actual">1</field>
	        <field name="implementation">standard</field>
	    </record>
	    
	</data>
</odoo> 